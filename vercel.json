{
  "buildCommand": "npm run build:mock",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api-proxy/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api-proxy/[...path].ts": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
