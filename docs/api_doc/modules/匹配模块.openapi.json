  "paths": {
    "/api/matching/{eventId}/extract-keywords": {
      "get": {
        "summary": "提取用户关键词条",
        "deprecated": false,
        "description": "从参与者的个人信息、兴趣爱好等字段中提取关键词",
        "operationId": "extractKeywords",
        "tags": [
          "关键词提取"
        ],
        "parameters": [
          {
            "name": "eventId",
            "in": "path",
            "description": "活动ID",
            "required": true,
            "example": "",
            "schema": {
              "type": "string",
              "format": "uuid",
              "example": "d1375b03-e2fe-418d-b096-9064eae0d1b9"
            }
          },
          {
            "name": "apifoxToken",
            "in": "header",
            "description": "",
            "example": "{{apifoxToken}}",
            "schema": {
              "type": "string",
              "default": "{{apifoxToken}}"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "提取成功",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "example": true
                    },
                    "message": {
                      "type": "string",
                      "example": "关键词提取成功"
                    },
                    "keywords": {
                      "type": "object",
                      "description": "各参与者的关键词映射",
                      "additionalProperties": {
                        "type": "array",
                        "items": {
                          "type": "string"
                        }
                      },
                      "x-apifox-orders": [],
                      "properties": {},
                      "x-apifox-ignore-properties": [],
                      "example": {
                        "user_001": [
                          "户外",
                          "摄影",
                          "徒步"
                        ],
                        "user_002": [
                          "音乐",
                          "阅读",
                          "咖啡"
                        ]
                      }
                    }
                  },
                  "x-apifox-orders": [
                    "success",
                    "message",
                    "keywords"
                  ],
                  "x-apifox-ignore-properties": []
                },
                "example": {
                  "success": true,
                  "message": "关键词提取成功",
                  "keywords": {
                    "user_001": [
                      "户外",
                      "摄影",
                      "徒步"
                    ],
                    "user_002": [
                      "音乐",
                      "阅读",
                      "咖啡"
                    ],
                    "user_003": [
                      "运动",
                      "健身",
                      "篮球"
                    ]
                  }
                }
              }
            },
            "headers": {},
            "x-apifox-name": "成功"
          }
        },
        "security": [],
        "x-apifox-folder": "关键词提取",
        "x-apifox-status": "released",
        "x-run-in-apifox": "https://app.apifox.com/web/project/7269221/apis/api-366709667-run"
      }
    }
  },
  "components": {
    "schemas": {
      "ErrorResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean",
            "example": false
          },
          "message": {
            "type": "string",
            "description": "错误信息",
            "example": "活动不存在"
          },
          "code": {
            "type": "string",
            "description": "错误代码",
            "example": "NOT_FOUND"
          }
        },
        "required": [
          "success",
          "message"
        ],
        "x-apifox-orders": [
          "success",
          "message",
          "code"
        ],
        "x-apifox-ignore-properties": [],
